<?xml version="1.0"?>
<h:html xmlns="http://www.w3.org/2002/xforms" xmlns:h="http://www.w3.org/1999/xhtml" xmlns:ev="http://www.w3.org/2001/xml-events" xmlns:xsd="http://www.w3.org/2001/XMLSchema" xmlns:jr="http://openrosa.org/javarosa" xmlns:orx="http://openrosa.org/xforms">
  <h:head>
    <h:title>Stock Refill Request</h:title>
    <model>
      <instance>
        <stock_refill_request id="vht_consumption_log" prefix="J1!vht_consumption_log!" delimiter="#" version="2021/04/23">
          <inputs>
            <meta>
              <location>
                <lat/>
                <long/>
                <error/>
                <message/>
              </location>
            </meta>
            <source/>
            <source_id/>
            <contact>
              <_id/>
              <name/>
              <contact>
                <name/>
                <phone/>
                <parent>
                  <_id/>
                  <parent>
                    <name/>
                  </parent>
                </parent>
              </contact>
            </contact>
          </inputs>
          <refill_group>
            <stock_status/>
            <known_requested_items/>
            <act_quantity_requested/>
            <zinc_quantity_requested/>
            <amoxicillin_quantity_requested/>
            <condoms_quantity_requested/>
            <contraceptives_quantity_requested/>
            <sayana_quantity_requested/>
            <other_choices jr:template="">
              <any_other/>
              <other_quantity_requested/>
            </other_choices>
          </refill_group>
          <meta tag="hidden">
            <instanceID/>
          </meta>
        </stock_refill_request>
      </instance>
      <instance id="contact-summary"/>
      <bind nodeset="/stock_refill_request/inputs" relevant="./source = 'user'"/>
      <bind nodeset="/stock_refill_request/inputs/source" type="string"/>
      <bind nodeset="/stock_refill_request/inputs/source_id" type="string"/>
      <bind nodeset="/stock_refill_request/inputs/contact/_id" type="db:health_center"/>
      <bind nodeset="/stock_refill_request/inputs/contact/name" type="string"/>
      <bind nodeset="/stock_refill_request/inputs/contact/contact/name" type="string"/>
      <bind nodeset="/stock_refill_request/inputs/contact/contact/phone" type="string"/>
      <bind nodeset="/stock_refill_request/inputs/contact/contact/parent/_id" type="string"/>
      <bind nodeset="/stock_refill_request/inputs/contact/contact/parent/parent/name" type="string"/>
      <bind nodeset="/stock_refill_request/refill_group/stock_status" type="select1" required="true()"/>
      <bind nodeset="/stock_refill_request/refill_group/known_requested_items" type="select" relevant=" /stock_refill_request/refill_group/stock_status ='yes'" required="true()"/>
      <bind nodeset="/stock_refill_request/refill_group/act_quantity_requested" type="int" relevant="selected( /stock_refill_request/refill_group/known_requested_items , 'ACT')" jr:constraintMsg="Value should be greater 0 and less than 100" required="true()" constraint=".&gt;=0 and .&lt;100"/>
      <bind nodeset="/stock_refill_request/refill_group/zinc_quantity_requested" type="int" relevant="selected( /stock_refill_request/refill_group/known_requested_items , 'zinc')" jr:constraintMsg="Value should be greater 0 and less than 101" required="true()" constraint=".&gt;=0 and .&lt;100"/>
      <bind nodeset="/stock_refill_request/refill_group/amoxicillin_quantity_requested" type="int" relevant="selected( /stock_refill_request/refill_group/known_requested_items , 'amoxicillin')" jr:constraintMsg="Value should be greater 0 and less than 102" required="true()" constraint=".&gt;=0 and .&lt;100"/>
      <bind nodeset="/stock_refill_request/refill_group/condoms_quantity_requested" type="int" relevant="selected( /stock_refill_request/refill_group/known_requested_items , 'condoms')" jr:constraintMsg="Value should be greater 0 and less than 103" required="true()" constraint=".&gt;=0 and .&lt;100"/>
      <bind nodeset="/stock_refill_request/refill_group/contraceptives_quantity_requested" type="int" relevant="selected( /stock_refill_request/refill_group/known_requested_items , 'contraceptives')" jr:constraintMsg="Value should be greater 0 and less than 104" required="true()" constraint=".&gt;=0 and .&lt;100"/>
      <bind nodeset="/stock_refill_request/refill_group/sayana_quantity_requested" type="int" relevant="selected( /stock_refill_request/refill_group/known_requested_items , 'sayana')" jr:constraintMsg="Value should be greater 0 and less than 105" required="true()" constraint=".&gt;=0 and .&lt;100"/>
      <bind nodeset="/stock_refill_request/refill_group/other_choices" relevant="selected( /stock_refill_request/refill_group/known_requested_items , 'other')"/>
      <bind nodeset="/stock_refill_request/refill_group/other_choices/any_other" type="string" relevant="selected( /stock_refill_request/refill_group/known_requested_items , 'other')" required="true()" jr:constraintMsg="Value should be greater 0 and less than 100"/>
      <bind nodeset="/stock_refill_request/refill_group/other_choices/other_quantity_requested" type="int" jr:constraintMsg="Value should be greater 0 and less than 100" required="true()" constraint=".&gt;=0 and .&lt;100"/>
      <bind nodeset="/stock_refill_request/meta/instanceID" type="string" readonly="true()" calculate="concat('uuid:', uuid())"/>
    </model>
  </h:head>
  <h:body class="pages">
    <group appearance="field-list" ref="/stock_refill_request/inputs">
      <label>CHV Area</label>
      <input appearance="hidden" ref="/stock_refill_request/inputs/source">
        <label>Source</label>
        <hint>user</hint>
      </input>
      <input appearance="hidden" ref="/stock_refill_request/inputs/source_id">
        <label>Source ID</label>
      </input>
      <group ref="/stock_refill_request/inputs/contact">
        <input appearance="db-object" ref="/stock_refill_request/inputs/contact/_id">
          <label>What is the CHv's area?</label>
        </input>
        <input appearance="hidden" ref="/stock_refill_request/inputs/contact/name">
          <label>Name</label>
        </input>
        <group ref="/stock_refill_request/inputs/contact/contact">
          <input appearance="hidden" ref="/stock_refill_request/inputs/contact/contact/name">
            <label>CHV Name</label>
          </input>
          <input appearance="hidden" ref="/stock_refill_request/inputs/contact/contact/phone">
            <label>Phone</label>
          </input>
          <group ref="/stock_refill_request/inputs/contact/contact/parent">
            <label>NO LABEL</label>
            <input appearance="db-object" ref="/stock_refill_request/inputs/contact/contact/parent/_id">
              <label>NO LABEL</label>
            </input>
            <group ref="/stock_refill_request/inputs/contact/contact/parent/parent">
              <label>NO LABEL</label>
              <input ref="/stock_refill_request/inputs/contact/contact/parent/parent/name">
                <label>NO LABEL</label>
              </input>
            </group>
          </group>
        </group>
      </group>
    </group>
    <group appearance="field-list" ref="/stock_refill_request/refill_group">
      <label>Stock Refill</label>
      <select1 ref="/stock_refill_request/refill_group/stock_status">
        <label>Do you have low stock for some of the items?</label>
        <item>
          <label>Yes</label>
          <value>yes</value>
        </item>
        <item>
          <label>No</label>
          <value>no</value>
        </item>
      </select1>
      <select appearance="horizontal" ref="/stock_refill_request/refill_group/known_requested_items">
        <label>Select the item you need to refill.</label>
        <item>
          <label>First Line Anti-Malarial (ACT â€“ Coartem)</label>
          <value>ACT</value>
        </item>
        <item>
          <label>Zinc Co-pack</label>
          <value>zinc</value>
        </item>
        <item>
          <label>Amoxicillin Dispersible</label>
          <value>amoxicillin</value>
        </item>
        <item>
          <label>Condoms</label>
          <value>condoms</value>
        </item>
        <item>
          <label>Emergency Contraceptives</label>
          <value>contraceptives</value>
        </item>
        <item>
          <label>Sayana Press</label>
          <value>sayana</value>
        </item>
        <item>
          <label>Other</label>
          <value>other</value>
        </item>
      </select>
      <input ref="/stock_refill_request/refill_group/act_quantity_requested">
        <label>Quantity requested ACT</label>
      </input>
      <input ref="/stock_refill_request/refill_group/zinc_quantity_requested">
        <label>Quantity requested Zinc</label>
      </input>
      <input ref="/stock_refill_request/refill_group/amoxicillin_quantity_requested">
        <label>Quantity requested Amoxicillin</label>
      </input>
      <input ref="/stock_refill_request/refill_group/condoms_quantity_requested">
        <label>Quantity requested Condoms</label>
      </input>
      <input ref="/stock_refill_request/refill_group/contraceptives_quantity_requested">
        <label>Quantity requested Contraceptives</label>
      </input>
      <input ref="/stock_refill_request/refill_group/sayana_quantity_requested">
        <label>Quantity requested Sayana Press</label>
      </input>
      <group ref="/stock_refill_request/refill_group/other_choices">
        <label></label>
        <repeat nodeset="/stock_refill_request/refill_group/other_choices" appearance="field-list">
          <input ref="/stock_refill_request/refill_group/other_choices/any_other">
            <label>Specify Other</label>
          </input>
          <input ref="/stock_refill_request/refill_group/other_choices/other_quantity_requested">
            <label>Quantity requested <output value=" /stock_refill_request/refill_group/other_choices/any_other "/></label>
          </input>
        </repeat>
      </group>
    </group>
  </h:body>
</h:html>
